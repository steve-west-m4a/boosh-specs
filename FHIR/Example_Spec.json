  "Download_Form_Settings": {
    "Rows": [
      {
        "Column_Name": "Order Number",
        "Download_Source_Name": "Standard Purchase Orders",
        "Implemented": true,
        "Is_Mandatory": false,
        "Column_SubSection": "Headers",
        "Control_Type": "LOV",
        "Support_Control": "Search",
        "Control_Column": "Left",
        "Download_Parameter": "OrderNumber",
        "Download_Section_Name": "Headers",
        "Download_Data_Type": "Text",
        "Report_Type": "OBIEE",
        "Report_Path": "/Custom/More4apps-Procurement/Procurement/Purchase Orders/Cancel Purchase Orders/CANPO_Order_Number_REPORT.xdo",
        "Report_Tag": "G_1",
        "Report_Restrict_Field": "ORDER_NUMBER",
        "Report_Search_Field": "ORDER_NUMBER_SEARCH",
        "Report_Validation_Field": "ORDER_NUMBER",
        "Report_SQL": "Select style.DISPLAY_NAME STYLE_NAME, header.PO_HEADER_ID, legalu.BU_NAME REQUISITIONING_BU, legalb.LEGAL_ENTITY_NAME SOLD_TO_LEGAL_ENTITY, header.SEGMENT1 ORDER_NUMBER, header.DOCUMENT_STATUS, sups.vendor_name SUPPLIER, supsite.Vendor_site_code SUPPLIER_SITE, term.name PAYMENT_TERM, header.NOTE_TO_VENDOR, header.NOTE_TO_RECEIVER FROM PO_HEADERS_ALL header, PO_DOC_STYLE_LINES_VL style, FUN_BU_USAGES_V legalu, FUN_NAMES_BUSINESS_UNITS_V legalb, poz_suppliers_v sups, po_supplier_sites_val_v supsite, ap_terms_val_v term  WHERE  header.style_id = style.style_id(+) AND header.SOLDTO_LE_ID = legalb.LEGAL_ENTITY_ID AND legalu.module_key = 'REQUISITIONING_BF' AND legalu.BUSINESS_UNIT_ID = legalb.bu_id AND sups.vendor_id(+) = header.vendor_id AND header.vendor_site_id = supsite.vendor_site_id(+) AND term.term_ID(+) = header. terms_ID AND header.REQ_BU_ID = legalu.BUSINESS_UNIT_ID AND header.type_lookup_code = style.document_subtype(+) AND (header.segment1 LIKE (:ORDER_NUMBER_SEARCH) OR header.segment1 IN (:ORDER_NUMBER)) and bu_id in ( SELECT   BusinessUnitUsage.BUSINESS_UNIT_ID FROM   FUN_BU_USAGES_V BusinessUnitUsage,   FUN_BUSINESS_FUNCTIONS_B BusinessFunction WHERE   BusinessUnitUsage.MODULE_ID = BusinessFunction.BUSINESS_FUNCTION_ID   AND BusinessFunction.BUSINESS_FUNCTION_CODE = 'REQUISITIONING_BF') AND ROWNUM <= 10000 ORDER BY ORDER_NUMBER",
        "Report_Clear_Cache": true
      },
      {
        "Column_Name": "Procurement BU",
        "Download_Source_Name": "Standard Purchase Orders",
        "Implemented": true,
        "Is_Mandatory": true,
        "Column_SubSection": "Headers",
        "Control_Type": "LOV",
        "Control_Column": "Right",
        "Download_Parameter": "ProcurementBU",
        "Download_Section_Name": "Headers",
        "Download_Data_Type": "Text",
        "Report_Type": "OBIEE",
        "Report_Path": "/Custom/More4apps-Procurement/Procurement/Purchase Orders/Cancel Purchase Orders/CANPO_Procurement_BU_DLF_REPORT.xdo",
        "Report_Tag": "G_1",
        "Report_Restrict_Field": "PROCUREMENT_BU",
        "Report_Search_Field": "PROCUREMENT_BU_SEARCH",
        "Report_Validation_Field": "PROCUREMENT_BU",
        "Report_SQL": "SELECT\n  BusinessUnitUsage.MODULE_ID,\n  BusinessUnit.BU_ID,\n  BusinessUnit.BU_NAME AS PROCUREMENT_BU,\n  BusinessUnit.DEFAULT_CURRENCY_CODE\nFROM\n  FUN_BU_USAGES_V BusinessUnitUsage,\n  FUN_ALL_BUSINESS_UNITS_V BusinessUnit,\n  FUN_BUSINESS_FUNCTIONS_B BusinessFunction\nWHERE\n  BusinessUnitUsage.BUSINESS_UNIT_ID = BusinessUnit.BU_ID\n  AND BusinessUnitUsage.MODULE_ID = BusinessFunction.BUSINESS_FUNCTION_ID\n  AND BusinessFunction.BUSINESS_FUNCTION_CODE = 'PROCUREMENT_BF'\n  AND (\n    BusinessUnit.BU_NAME IN (:PROCUREMENT_BU)\n    OR BusinessUnit.BU_NAME LIKE (:PROCUREMENT_BU_SEARCH)\n  )"
      },
      {
        "Column_Name": "Supplier",
        "Download_Source_Name": "Standard Purchase Orders",
        "Implemented": true,
        "Is_Mandatory": false,
        "Column_SubSection": "Headers",
        "Control_Type": "LOV",
        "Support_Control": "Search",
        "Control_Column": "Left",
        "Download_Parameter": "Supplier",
        "Download_Section_Name": "Headers",
        "Download_Data_Type": "Text",
        "Report_Type": "OBIEE",
        "Report_Path": "/Custom/More4apps-Procurement/Procurement/Purchase Orders/Cancel Purchase Orders/CANPO_Supplier_DLF_REPORT.xdo",
        "Report_Tag": "G_1",
        "Report_Restrict_Field": "SUPPLIER",
        "Report_Search_Field": "SUPPLIER_SEARCH",
        "Report_Dependencies": "$(PROCUREMENT_BU_ID|Procurement BU|BU_ID)",
        "Report_Validation_Field": "PARTY_NAME",
        "Report_SQL": "SELECT distinct supplier.vendor_id, Party.PARTY_ID AS PARTY_ID, segment1 \"Supplier number\", Party.PARTY_NAME AS PARTY_NAME ,DECODE(count (*) over(partition by supplier.vendor_id),1, sites.vendor_site_code, null) default_site, sites.prc_bu_id PROCUREMENT_BU_ID FROM poz_suppliers supplier, HZ_PARTIES Party ,po_supplier_sites_val_v sites WHERE (Supplier.PARTY_ID = Party.PARTY_ID) AND enabled_flag = 'Y' AND sysdate BETWEEN NVL(start_date_active, sysdate-1) AND NVL(end_date_active, sysdate+1) AND sites.vendor_id = supplier.vendor_id AND (Party.PARTY_NAME LIKE(:supplier_search) OR Party.PARTY_NAME IN( :supplier )) and (sites.prc_bu_id IN (:PROCUREMENT_BU_ID) or :PROCUREMENT_BU_ID is null) AND ROWNUM <= 10000 ORDER BY upper(party_name)"
      },
      {
        "Column_Name": "Creation Date From",
        "Download_Source_Name": "Standard Purchase Orders",
        "Implemented": true,
        "Is_Mandatory": false,
        "Column_SubSection": "Headers",
        "Control_Type": "DateTime",
        "Control_Column": "Left",
        "Download_Parameter": "CreationDate",
        "Download_Section_Name": "Headers",
