        "Control_Type": "DateTime",
        "Control_Column": "Right",
        "Download_Parameter": "CreationDate",
        "Download_Section_Name": "Headers",
        "Download_Data_Type": "DateTime",
        "Validation_Data_Type": "DateTime",
        "From_Column_Name": "Creation Date From"
      },
      {
        "Column_Name": "Order Status",
        "Download_Source_Name": "Standard Purchase Orders",
        "Implemented": true,
        "Is_Mandatory": false,
        "Column_SubSection": "Headers",
        "Control_Type": "LOV",
        "Control_Column": "Left",
        "Download_Parameter": "Status",
        "Download_Section_Name": "Headers",
        "Download_Data_Type": "Text",
        "Report_Type": "OBIEE",
        "Report_Path": "/Custom/More4apps-Procurement/Procurement/Purchase Orders/Cancel Purchase Orders/CANPO_Order_Status_DLF_REPORT.xdo",
        "Report_Tag": "G_1",
        "Report_Restrict_Field": "ORDER_STATUS",
        "Report_Search_Field": "ORDER_STATUS_SEARCH",
        "Report_Validation_Field": "DISPLAYED_FIELD",
        "Report_SQL": "SELECT\n  MEANING DISPLAYED_FIELD,\n  MEANING STATUS,\n  LOOKUP_CODE\nFROM\n  fnd_lookup_values_vl\nWHERE\n  ENABLED_FLAG = 'Y'\n  AND  sysdate BETWEEN NVL(start_date_active, sysdate-1) AND NVL(end_date_active, sysdate+1)\n  AND LOOKUP_TYPE = 'ORDER_STATUS'\n AND LOOKUP_CODE not in ('CANCELED','FINALLY CLOSED','INCOMPLETE','ON HOLD') /*Case45699*/\n  AND (\n    MEANING LIKE (:ORDER_STATUS_SEARCH)\n    OR\n    MEANING IN (:ORDER_STATUS)\n  )\n  AND ROWNUM <= 10000\nORDER BY\n  MEANING"
      },
      {
        "Column_Name": "Buyer",
        "Download_Source_Name": "Standard Purchase Orders",
        "Implemented": true,
        "Is_Mandatory": false,
        "Column_SubSection": "Headers",
        "Control_Type": "LOV",
        "Support_Control": "Search",
        "Control_Column": "Right",
        "Download_Parameter": "Buyer",
        "Download_Section_Name": "Headers",
        "Download_Data_Type": "Text",
        "Report_Type": "OBIEE",
        "Report_Path": "/Custom/More4apps-Procurement/Procurement/Purchase Orders/Cancel Purchase Orders/CANPO_Buyer_DLF_REPORT.xdo",
        "Report_Tag": "G_1",
        "Report_Restrict_Field": "BUYER",
        "Report_Search_Field": "BUYER_SEARCH",
        "Report_Dependencies": "$(PROCUREMENT_BU_ID|Procurement BU|BU_ID)",
        "Report_Validation_Field": "BUYER",
        "Report_SQL": "SELECT distinct PersonNameDPEO.LIST_NAME Buyer, BuyerPEO.AGENT_ID , EmailAddressPEO.EMAIL_ADDRESS, BU.BU_NAME DEFAULT_REQ_BU, BuyerPEO.prc_bu_id PROCUREMENT_BU_ID FROM PO_AGENT_ACCESSES BuyerPEO, PER_ALL_PEOPLE_F PersonDPEO, PER_PERSON_NAMES_F_V PersonNameDPEO, PER_EMAIL_ADDRESSES EmailAddressPEO, PER_PERIODS_OF_SERVICE PeriodOfServicePEO, PER_ALL_ASSIGNMENTS_M AssignmentDPEO, HR_ORGANIZATION_UNITS_F_TL PrcBuPEO, PER_DEPARTMENTS DepartmentDPEO ,FUN_ALL_BUSINESS_UNITS_V BU WHERE PrcBuPEO.ORGANIZATION_ID = BuyerPEO.PRC_BU_ID AND TRUNC(SYSDATE) BETWEEN PrcBuPEO.EFFECTIVE_START_DATE AND PrcBuPEO.EFFECTIVE_END_DATE AND PrcBuPEO.LANGUAGE = USERENV('LANG') AND BU.BU_ID(+) = DEFAULT_REQ_BU_ID AND BuyerPEO.AGENT_ID = PeriodOfservicePEO.PERSON_ID AND PeriodOfservicePEO.PRIMARY_FLAG = 'Y' AND AssignmentDPEO.PERSON_ID = BuyerPEO.AGENT_ID AND AssignmentDPEO.period_of_service_id = PeriodOfservicePEO.period_of_service_id AND AssignmentDPEO.primary_flag = 'Y' AND AssignmentDPEO.EFFECTIVE_LATEST_CHANGE = 'Y' AND sysdate BETWEEN assignmentdpeo.effective_start_date AND assignmentdpeo.effective_end_date AND BuyerPEO.AGENT_ID = PersonDPEO.PERSON_ID AND sysdate BETWEEN PersonDPEO.EFFECTIVE_START_DATE AND PersonDPEO.EFFECTIVE_END_DATE AND (EmailAddressPEO.EMAIL_ADDRESS_ID (+) = PersonDPEO.PRIMARY_EMAIL_ID) AND (BuyerPEO.AGENT_ID = PersonNameDPEO.PERSON_ID AND sysdate BETWEEN PersonNameDPEO.EFFECTIVE_START_DATE AND PersonNameDPEO.EFFECTIVE_END_DATE) AND (DepartmentDPEO.ORGANIZATION_ID (+) = AssignmentDPEO.ORGANIZATION_ID AND sysdate BETWEEN DepartmentDPEO.EFFECTIVE_START_DATE(+) AND DepartmentDPEO.EFFECTIVE_END_DATE(+)) AND (PersonNameDPEO.LIST_NAME LIKE (:BUYER_SEARCH) OR PersonNameDPEO.LIST_NAME IN (:BUYER)) AND BuyerPEO.access_action_code = 'MANAGE_PURCHASE_ORDERS' AND (BuyerPEO.prc_bu_id in (:PROCUREMENT_BU_ID) or :PROCUREMENT_BU_ID is null) AND BuyerPEO.allowed_flag = 'Y' AND BuyerPEO.active_flag = 'Y' AND ROWNUM <= 10000 order by PersonNameDPEO.LIST_NAME"
      }
    ],
